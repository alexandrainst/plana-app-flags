{
  "$schema": "https://raw.githubusercontent.com/open-feature/schemas/main/json/flagd-definitions.json",
  "flags": {
    "singleRouteMode": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "on"
    },
    "toggleDebug": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "off",
      "targeting": {
        "if": [
          {
            "testserver": {
              "in": [
                "testserver",
                {
                  "var": [
                    "systemId"
                  ]
                }
              ]
            }
          },
          "on",
          "off"
        ]
      }
    }
  }
}