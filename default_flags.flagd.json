{
  "$schema": "https://flagd.dev/schema/v0/flags.json",
  "flags": {
    "singleRouteMode": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "on",
      "targeting": {
        "if": [
          {
            "==": [
              "testserver",
              {
                "var": "systemId"
              }
            ]
          },
          "on",
          "off"
        ]
      }
    },
    "allowDebug": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "off",
      "targeting": {
        "if": [
          {
            "==": [
              "testserver",
              {
                "var": "systemId"
              }
            ]
          },
          "on",
          "off"
        ]
      }
    },
    "allowBiometricSignin": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "off"
    },
    "showAllRoutes": {
      "state": "ENABLED",
      "variants": {
        "on": true,
        "off": false
      },
      "defaultVariant": "off",
      "targeting": {
        "if": [
          {
            "==": [
              "testserver2",
              {
                "var": "systemId"
              }
            ]
          },
          "on",
          "off"
        ]
      }
    }
  }
}
