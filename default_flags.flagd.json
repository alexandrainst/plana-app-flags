{
  "$schema": "https://flagd.dev/schema/v0/flags.json",
  "flags": {
    "singleRouteMode": {
      "state": "ENABLED",
      "variants": {
        "true": true,
        "false": false
      },
      "defaultVariant": "false",
      "targeting": {
        "==": [
          "testserver",
          {
            "var": "systemId"
          }
        ]
      }
    },
    "allowDebug": {
      "state": "ENABLED",
      "variants": {
        "true": true,
        "false": false
      },
      "defaultVariant": "false",
      "targeting": {
        "==": [
          "testserver",
          {
            "var": "systemId"
          }
        ]
      }
    },
    "allowBiometricSignin": {
      "state": "ENABLED",
      "variants": {
        "true": true,
        "false": false
      },
      "defaultVariant": "false"
    },
    "showAllRoutes": {
      "state": "ENABLED",
      "variants": {
        "true": true,
        "false": false
      },
      "defaultVariant": "false",
      "targeting": {
        "or": [
          {
            "==": [
              "testserver2",
              {
                "var": "systemId"
              }
            ]
          },
          {
            "==": [
              "testserver1",
              {
                "var": "systemId"
              }
            ]
          }
        ]
      }
    }
  }
}
